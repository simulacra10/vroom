/*
  CUSTOM MOBILE OVERRIDES  full-width + stacked cols
*/
@media (max-width: 767px) {
  /* make all your main wrappers truly fluid */
  .container,
  .container-fluid,
  .content-wrapper,
  #main,
  section,
  article {
    width:       100%     !important;
    max-width:   100%     !important;
    margin:      0 auto   !important;
    padding:     0 1rem   !important;
  }

  /* force any row/layout to stack vertically */
  .row,
  .layout {
    display:         flex        !important;
    flex-direction:  column      !important;
    gap:             1rem        !important;
  }

  /* ensure any col-* children go full width */
  .row > [class*="col-"] {
    flex:        0 0 100%        !important;
    max-width:   100%            !important;
  }
}

